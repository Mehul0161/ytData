{
  "version": 2,
  "name": "youtube-analytics-backend",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "YOUTUBE_API_KEY": "@youtube_api_key"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
} 